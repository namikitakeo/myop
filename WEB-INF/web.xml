<web-app>
  <servlet>
    <servlet-name>login</servlet-name>
    <servlet-class>bean.login</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>login</servlet-name>
    <url-pattern>/login</url-pattern>
  </servlet-mapping>
  <servlet>
    <servlet-name>consent</servlet-name>
    <servlet-class>bean.consent</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>consent</servlet-name>
    <url-pattern>/consent</url-pattern>
  </servlet-mapping>
  <servlet>
    <servlet-name>error</servlet-name>
    <servlet-class>bean.error</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>error</servlet-name>
    <url-pattern>/error</url-pattern>
  </servlet-mapping>
  <servlet>
    <servlet-name>logout</servlet-name>
    <servlet-class>bean.logout</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>logout</servlet-name>
    <url-pattern>/logout</url-pattern>
  </servlet-mapping>
  <servlet>
    <servlet-name>discovery</servlet-name>
    <servlet-class>discovery</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>discovery</servlet-name>
    <url-pattern>/.well-known/openid-configuration</url-pattern>
  </servlet-mapping>
  <servlet>
    <servlet-name>keys</servlet-name>
    <servlet-class>keys</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>keys</servlet-name>
    <url-pattern>/jwk_uri</url-pattern>
  </servlet-mapping>
  <servlet>
    <servlet-name>authorize</servlet-name>
    <servlet-class>authorize</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>authorize</servlet-name>
    <url-pattern>/authorize</url-pattern>
  </servlet-mapping>
</web-app>
